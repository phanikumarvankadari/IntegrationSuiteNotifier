{
  "name": "IntegrationSuiteNotifier",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.1.1",
    "@sap-cloud-sdk/util": "^4.1.1",
    "@sap/cds": "^9",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.9.1",
    "express": "^4",
    "hdb": "^2.25.1",
    "passport": "^0.7.0",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/resilience": "^3"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      },
      "sap_cloud_integration_monitoring_api": {
        "kind": "odata-v2",
        "model": "srv/external/sap_cloud_integration_monitoring_api",
        "[production]": {
          "credentials": {
            "destination": "sap_cloud_integration_monitoring_api"
          }
        },
        "csrf": true,
        "csrfInBatch": true
      }
    }
  },
  "lcap": true
}
